{
    "name": "@threepipe/plugin-svg-renderer",
    "description": "Plugins for SVG Rendering of 3d objects for Threepipe",
    "version": "0.2.4",
    "devDependencies": {
        "@svgdotjs/svg.js": "^3.2.0",
        "@svgdotjs/svg.topath.js": "^2.0.3",
        "arrangement-2d-js": "github:LokiResearch/arrangement-2d-js",
        "fast-triangle-triangle-intersection": "^1.0.7",
        "flatbush": "^4.4.0",
        "isect": "^3.0.2",
        "three-mesh-bvh": "^0.7.4",
        "xml-formatter": "^2.6.1",
        "three": "file:./../../node_modules/three"
    },
    "dependencies": {
        "threepipe": "file:./../../src/"
    },
    "peerDependencies": {
        "threepipe": "^0.1.0"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./dist/": {
            "import": "./dist/",
            "require": "./dist/"
        }
    },
    "clean-package": {
        "remove": [
            "clean-package",
            "scripts",
            "devDependencies",
            "//"
        ],
        "replace": {
            "dependencies": {}
        }
    },
    "type": "module",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "src", "README.md", "CHANGELOG.md", "docs"
    ],
    "scripts": {
        "new:pack": "npm run prepare 1>&2 && npm run docs 1>&2 && clean-package && npm pack --silent && clean-package restore",
        "new:publish": "git diff --exit-code --name-only HEAD * && PKG=$(npm run -s new:pack) && npm publish --access public \"$PKG\" && git tag $npm_package_name-$npm_package_version",
        "prepare": "npm run build",
        "build": "rimraf dist && vite build",
        "dev": "NODE_ENV=development vite build --watch",
        "docs": "rimraf docs && npx typedoc --skipErrorChecking"
    },
    "author": "repalash <palash@shaders.app>",
    "license": "GPLV3",
    "keywords": [
        "three",
        "three.js",
        "threepipe",
        "svg",
        "rendering",
        "vector-graphics"
    ],
    "bugs": {
        "url": "https://github.com/repalash/threepipe/issues"
    },
    "homepage": "https://github.com/repalash/threepipe#readme",
    "repository": {
        "type": "git",
        "url": "git://github.com/repalash/threepipe.git",
        "directory": "plugins/svg-renderer"
    }
}
